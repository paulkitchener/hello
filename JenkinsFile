pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building..'
		git 'https://github.com/paulkitchener/hello.git' -b pk
		sh 'javac HelloWorld.java'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
		sh './run.sh'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
